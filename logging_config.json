{
    "version": 1,
    "formatters": {
      "simple": {
        "format": "%(name)s -> %(levelname)s -> %(asctime)s -> %(message)s",
        "datefmt": "%Y-%m-%d %H:%M:%S"
      }
    },
    "handlers": {
      "file_handler": {
        "class": "logging.FileHandler",
        "formatter": "simple",
        "filename": "example.log",
        "level": "INFO"
      },
      "console_handler": {
        "class": "logging.StreamHandler",
        "formatter": "simple",
        "level": "INFO"
      }
    },
    "loggers": {
      "my_module": {
        "level": "INFO",
        "handlers": ["console_handler"]
      }
    },
    "root": {
      "level": "INFO",
      "handlers": ["file_handler", "console_handler"]
    }
  }
  